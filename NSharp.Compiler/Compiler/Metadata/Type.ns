    public fn Handle EntityHandle
    public fn TypeDefinitionHandle TypeDefinitionHandle : Handle as TypeDefinitionHandle
    public fn TypeReferenceHandle TypeReferenceHandle : Handle as TypeReferenceHandle
    public fn Namespace str = str.Empty
    public fn Name str = str.Empty
