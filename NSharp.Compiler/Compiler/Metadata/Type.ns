use System.Reflection.Metadata

ns NSharp.Compiler.Metadata

public class Type has IType
    public fn new(assembly Assembly, type TypeDefinition)
        var nspace = assembly.Meta.GetString(type.Namespace)
        var name = assembly.Meta.GetString(type.Name)
        assembly.TypeResolver.AddType(str.IsNullOrEmpty(nspace) ? name : "{nspace}.{name}", this)
