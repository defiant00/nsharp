ns NSharp.Core.TypedTree

public class Using is Statement
    public fn Name IdentifierToken
    public fn Value Expression
    public fn Statements List{Statement} = new()

    public fn new(position Position, name IdentifierToken, value Expression) base(position)
        Name = name
        Value = value
    
    public over fn Accept(visitor ITypedTreeVisitor) is visitor.Visit(this)
