ns NSharp.Core.SyntaxTree

public class Delegate is Node
    public fn ReturnType ?Node
    public fn Name Node
    public fn GenericNames List{Node} = new()
    public fn Parameters List{Node} = new()

    public fn new(position Position, name Node) base(position) is Name = name
    
    public over fn Accept(visitor ISyntaxTreeVisitor) is visitor.Visit(this)
