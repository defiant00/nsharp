ns NSharp.Core.SyntaxTree

public class Enumeration is Statement
    public fn Name IdentifierToken
    public fn Values List{Statement}

    public fn new(position Position, name IdentifierToken, values ?List{Statement} = null) base(position)
        Name = name
        Values = values ?? new()
    
    public over fn Accept(visitor ISyntaxTreeVisitor) is visitor.Visit(this)
