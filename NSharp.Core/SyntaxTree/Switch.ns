ns NSharp.Core.SyntaxTree

public class Switch is Statement
    public fn LocalName ?IdentifierToken
    public fn LocalValue ?Expression
    public fn Value Expression
    public fn Cases List{Case} = new()

    public fn new(position Position, value Expression) base(position) is Value = value
    
    public over fn Accept(visitor ISyntaxTreeVisitor) is visitor.Visit(this)
