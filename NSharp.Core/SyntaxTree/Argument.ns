ns NSharp.Core.SyntaxTree

public class Argument is Node
    public fn Name ?Node
    public fn Modifiers List{Node}
    public fn Value Node

    public fn new(position Position, name ?Node, modifiers List{Node}, value Node) base(position)
        Name = name
        Modifiers = modifiers
        Value = value
    
    public over fn Accept(visitor ISyntaxTreeVisitor) is visitor.Visit(this)
