ns NSharp.Core.SyntaxTree

public class LocalConstant is Statement
    public fn Name IdentifierToken
    public fn Type Expression
    public fn Value Expression

    public fn new(position Position, name IdentifierToken, type Expression, value Expression) base(position)
        Name = name
        Type = type
        Value = value
    
    public over fn Accept(visitor ISyntaxTreeVisitor) is visitor.Visit(this)
