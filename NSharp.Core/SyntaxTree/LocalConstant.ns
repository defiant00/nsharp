ns NSharp.Core.SyntaxTree

public class LocalConstant is Node
    public fn Name Node
    public fn Type Node
    public fn Value Node

    public fn new(position Position, name Node, type Node, value Node) base(position)
        Name = name
        Type = type
        Value = value
    
    public over fn Accept(visitor ISyntaxTreeVisitor) is visitor.Visit(this)
