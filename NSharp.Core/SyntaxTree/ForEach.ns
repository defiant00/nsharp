ns NSharp.Core.SyntaxTree

public class ForEach is Statement
    public fn LocalName IdentifierToken
    public fn Value Expression
    public fn Statements List{Statement} = new()

    public fn new(position Position, localName IdentifierToken, value Expression) base(position)
        LocalName = localName
        Value = value
    
    public over fn Accept(visitor ISyntaxTreeVisitor) is visitor.Visit(this)
