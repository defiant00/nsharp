ns NSharp.Core.SyntaxTree

public enum ModifierType
    Public
    Protected
    Internal
    Private
    Abstract
    Extension
    Open
    Override
    Static

public class ModifierToken is Node
    public fn Value ModifierType

    public fn new(position Position, value ModifierType) base(position) is Value = value
    
    public over fn Accept(visitor ISyntaxTreeVisitor) is visitor.Visit(this)
