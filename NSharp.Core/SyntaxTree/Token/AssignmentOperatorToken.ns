ns NSharp.Core.SyntaxTree

public enum AssignmentOperatorType
    Assign
    Add
    Subtract
    Multiply
    Divide
    Modulus
    BitwiseAnd
    BitwiseOr
    BitwiseXor
    LeftShift
    RightShift
    NullCoalesce

public class AssignmentOperatorToken is Node
    public fn Value AssignmentOperatorType

    public fn new(position Position, value AssignmentOperatorType) base(position) is Value = value
    
    public over fn Accept(visitor ISyntaxTreeVisitor) is visitor.Visit(this)
